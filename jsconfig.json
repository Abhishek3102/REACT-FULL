{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2022",
        "jsx": "react-jsx",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": ".",
    "paths": {
      "styles/*": ["src/styles/*"]
    }
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}